/* declares vehicle id */
Vehicle.Id = 1;

/* declares vehicle category */
Vehicle.Category = "";

/* declares vehicle title */
Vehicle.Title = "Średni samochód gaśniczy";

/* declares vehicle pattern */
Vehicle.AddOrUpdatePattern(new string[] { "Średni samochód gaśniczy", "Ciężki samochód gaśniczy", "samochód pożarniczy", "samochody(-ów) pożarnicze(-ych)" });

/* regex-pattern need water */
Vehicle.AddOrUpdatePattern(@"([0-9]+) litrów wody", 1);